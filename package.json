{
  "name": "browsix",
  "version": "1.0.0",
  "description": "in-browser, multi-process operating system",
  "main": "lib/kernel/kernel.js",
  "scripts": {
    "needs-format": "prettier -l \"src/**/*.ts\" \"test/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/*.ts\"",
    "lint": "yarn run tslint -c tslint.json --project tsconfig.json",
    "build:pre": "node support/mkdirp.js lib-dist/kernel lib/kernel",
    "build:kernel": "tsc -p tsconfig.json",
    "build:browser": "rollup -c .rollup.kernel.js",
    "build:terminal": "node support/mkdirp.js app/elements lib/app/elements/browsix-terminal",
    "build:bin": "node support/mkdirp.js lib-dist/bin lib/bin",
    "build": "npm-run-all build:pre build:kernel build:browser build:terminal build:bin",
    "prepublishOnly": "make clean && npm run-script build"
  },
  "repository": {
    "url": "https://github.com/plasma-umass/browsix.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plasma-umass/browsix/issues"
  },
  "homepage": "https://github.com/plasma-umass/browsix",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "browserfs-browsix-tmp": "1.9.3",
    "chai": "^4.1.2",
    "karma": "^3.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.3",
    "mocha": "^4.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "rollup": "^0.68.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43"
  },
  "dependencies": {
    "@polymer/decorators": "^3.0.0",
    "@polymer/polymer": "^3.0.0-pre.13",
    "@types/filesystem": "0.0.28",
    "@types/polymer": "^1.2.7",
    "@webcomponents/webcomponentsjs": "^1.0.8",
    "dropbox": "~4.0.9",
    "node-binary-marshal": "^0.4.2",
    "term.js": "github:bpowers/term.js"
  },
  "engines": {
    "node": ">=6.11.4"
  }
}
