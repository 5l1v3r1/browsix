{
  "name": "browsix",
  "version": "1.0.0",
  "description": "in-browser, multi-process operating system",
  "main": "lib/kernel/kernel.js",
  "scripts": {
    "needs-format": "prettier -l \"src/*.ts\" \"test/*.ts\"",
    "format": "prettier --write \"src/*.ts\" \"test/*.ts\"",
    "lint": "yarn run tslint -c .tslint.json --project .tsconfig.lib-cjs.json",
    "test": "tsc -p .tsconfig.test.json && mocha",
    "build:pre": "node support/mkdirp.js build-rt lib build",
    "build:runtime0": "tsc -p .tsconfig.rt.json",
    "build:runtime1": "node support/build-runtime.js src/runtime.ts",
    "build:lib": "tsc -p .tsconfig.lib-cjs.json",
    "build:build": "tsc -p .tsconfig.lib.json",
    "build:browser": "rollup -c .rollup.lib.js",
    "build": "npm-run-all build:pre build:runtime0 build:runtime1 -p build:lib build:build -s build:browser",
    "prepublishOnly": "make clean && npm run-script build"
  },
  "repository": {
    "url": "https://github.com/plasma-umass/browsix.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plasma-umass/browsix/issues"
  },
  "homepage": "https://github.com/plasma-umass/browsix",
  "devDependencies": {
    "bfs-buffer": "^0.1.7",
    "bfs-path": "^0.1.2",
    "browser-sync": "^2.24.4",
    "browserfs-browsix-tmp": "0.5.15",
    "chai": "^4.1.2",
    "karma": "^3.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "mocha": "^4.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43"
  },
  "dependencies": {
    "@types/dropboxjs": "0.0.29",
    "@types/filesystem": "0.0.28",
    "node-binary-marshal": "^0.4.2",
    "term.js": "github:bpowers/term.js"
  },
  "engines": {
    "node": ">=6.11.4"
  }
}
